pipeline{
agent any 
tools{
	maven 'Maven'
}
stages{
	stage('Checkout'){
	steps{
		git branch:'master',url='https://github.com/saiashok0981/mymaven01.git'
		}
	}
	stage('Build'){
		steps{
			sh 'mvn clean package'
		}
	}
	stage('Test'){
		steps{
			sh 'mvn test'
		}
	}
	stage('Run Application'){
		steps{
			sh 'java -jar target/MyMavenjenkinsPipeline-1.0-SNAPSHOT.jar'
		}
	}
}
post{
	SUCCESS{
		echo 'build and deployement successful'
	}
	failure{
		echo 'Build failure'
	}
}
}
